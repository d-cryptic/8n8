model Credential {
	id String @id @default(auto()) @map("_id") @db.ObjectId
	title String
	platform String
	data Json
	userId String @db.ObjectId
	createdAt DateTime @default(now())
	updatedAt DateTime @updatedAt

	user User @relation(fields: [userId], references: [id], onDelete: Cascade)

	@@map("credentials")
}